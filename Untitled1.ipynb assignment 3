      "cell_type": "code",
      "source": [
        "import time\n",
        "import cProfile\n",
        "import pstats\n",
        "import io\n",
        "import tracemalloc\n",
        "def swap_numbers(a, b):\n",
        "    a = a + b\n",
        "    b = a - b\n",
        "    a = a - b\n",
        "    return a, b\n",
        "def run_timing(a, b):\n",
        "    print(\"=== Before Swapping ===\")\n",
        "    print(f\"a = {a}, b = {b}\\n\")\n",
        "\n",
        "    start = time.perf_counter()\n",
        "    a, b = swap_numbers(a, b)\n",
        "    end = time.perf_counter()\n",
        "\n",
        "    print(\"=== After Swapping ===\")\n",
        "    print(f\"a = {a}, b = {b}\\n\")\n",
        "    print(\"=== Execution Time ===\")\n",
        "    print(f\"Time taken : {end - start:.8f} seconds\\n\")\n",
        "def run_cpu_profile(a, b):\n",
        "    profiler = cProfile.Profile()\n",
        "    profiler.enable()\n",
        "    swap_numbers(a, b)\n",
        "    profiler.disable()\n",
        "\n",
        "    s = io.StringIO()\n",
        "    stats = pstats.Stats(profiler, stream=s).sort_stats(\"cumulative\")\n",
        "    stats.print_stats(10)\n",
        "\n",
        "    print(\"=== CPU Profiling (Top 10) ===\")\n",
        "    print(s.getvalue())\n",
        "def run_memory_profile(a, b):\n",
        "    tracemalloc.start()\n",
        "    swap_numbers(a, b)\n",
        "    current, peak = tracemalloc.get_traced_memory()\n",
        "    tracemalloc.stop()\n",
        "\n",
        "    print(\"=== Memory Profiling ===\")\n",
        "    print(f\"Current memory usage : {current / 10**6:.6f} MB\")\n",
        "    print(f\"Peak memory usage    : {peak / 10**6:.6f} MB\\n\")\n",
        "\n",
        "def main():\n",
        "    a = 10\n",
        "    b = 20\n",
        "    print(\"\\nSwapping Two Numbers Using Arithmetic Method\\n\")\n",
        "    run_timing(a, b)\n",
        "    run_cpu_profile(a, b)\n",
        "    run_memory_profile(a, b)\n",
        "\n",
        "if __name__ == \"__main__\":\n",
        "    main()"
      ],
